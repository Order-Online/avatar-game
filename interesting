<div id="exiting-customerUrl-id"></div>
<script>
// Enable profile loading when page override is enabled, or if global option is to enable
var enableProfileLoad,
    pageOverrideSetting = 'default';
if (pageOverrideSetting === 'default') {
    enableProfileLoad = Spectrum.Residential.WhiteListCustomerProfileParams.some(function(data) {
        return (data.fieldName === 'enableProfileLoad' && data.defaultValue === 'true');
    });
} else {
    enableProfileLoad = pageOverrideSetting === 'enable' ? true : false;
}


if(navigator.cookieEnabled && enableProfileLoad && (!/(?:^|.*;)\s*spectrum-existing-customer-profile\s*\=\s*/.test(document.cookie))) {
    var xmlHttp = Spectrum.HeadUtil.createXMLHttp();
    var existingCustomerUrl = "/bin/spectrum/residential/existingCustomerProfileLoader.js";

    var ip = Spectrum.HeadUtil.getURLParameter('ip');
    existingCustomerUrl += (ip != '' && ip != null) ? '?ip=' + ip : '?ip=';
    var network = Spectrum.HeadUtil.getURLParameter('network');
    existingCustomerUrl += (network != '' && network != null) ? '&network=' + network : '&network=';

    xmlHttp.open("get", existingCustomerUrl, false);
    xmlHttp.onreadystatechange = function(oEvent) {
        if (xmlHttp.readyState === Spectrum.Residential.XMLHttp.DONE) {
            if(xmlHttp.status === 200) {
                var response = xmlHttp.responseText;

                // only track duration on successful calls since the time could be much shorter if not all api calls are made
                if(response.indexOf(',') > -1) {
                    var responses = response.split(',');

                    // responses[0] is 'R','RES','C', or 'COM'
                    response = responses[0];

                    // response[1] is the duration of the existing Customer api call in milliseconds
                    Spectrum.HeadUtil.setExistingCustomerAPIDuration(responses[1]);
                }

                if(response === 'C' || response === 'COM') {
                    var redirectUrl = document.getElementById('exiting-customerUrl-id').getAttribute("exiting-redirect-url");
                    if(!Spectrum.HeadUtil.isEmpty(redirectUrl)) {
                        window.location = redirectUrl;
                    }
                }
            } else {
                console.error('Failed to load existing customer profile!');
            }
        }
    }
    xmlHttp.send(null);
}
</script>







