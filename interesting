<script type="text/javascript">
    var Spectrum = Spectrum || {};
    Spectrum.Residential = Spectrum.Residential || {};
    Spectrum.Residential.GlobalParamsMap = Spectrum.Residential.GlobalParamsMap || {};
    var baseUrl = "/bin/spectrum/residential/globalVariables.js";
    $.ajax({
          url: baseUrl,
          dataType: "json",
          async: false,
          timeout: 8000,
          success: function(data) {
              Spectrum.Residential.GlobalParamsMap = data || {};
          },
          error: function() {
              console.log("error received while servlet call");
              Spectrum.Residential.GlobalParamsMap = {};
          }
        });
</script>




    
